{
	"name": "galhardo-app",
	"version": "3.0.0",
	"description": "A Simple Web App Project To Learn how to program a Ecommerce and Subscriptions System",
	"main": "dist/server.js",
	"author": "Alex Galhardo <aleexgvieira@gmail.com>",
	"license": "MIT",
	"homepage": "https://github.com/AlexGalhardo/Galhardo-Ecommerce",
	"prisma": {
		"seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"scripts": {
		"start": "npm run build && node dist/server.js",
		"nodemon": "npx nodemon dist/server.js",
		"pm2": "npx pm2 start dist/server.js --name galhardo-app-api --interpreter node --watch",
		"dev": "concurrently \"npm run watch:compile\" \"node ./dist/server.js\"",
		"watch:compile": "npm run delete:build && npx swc src --out-dir dist && cp -r ./src/views dist/ && npx swc -w src --out-dir dist ",
		"watch:dev": "npx nodemon ./dist/server.js",
		"build": "npm run delete:build && swc src -d dist",
		"vitest": "vitest",
		"vitest:coverage": "vitest run --coverage",
		"fix:prettier": "npx prettier --write src/*.ts src/**/*.ts src/**/**/*.ts src/**/**/**/*.ts tests/**/*.ts",
		"fix:lint": "npx eslint . --fix",
		"precommit": "rm -rf dist/ && npm run fix:prettier && npm run fix:lint",
		"delete:build": "rm -rf dist",
		"postinstall": "npx prisma generate"
	},
	"dependencies": {
		"@faker-js/faker": "^7.6.0",
		"@prisma/client": "^4.8.0",
		"axios": "^1.2.2",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.1",
		"compression": "^1.7.4",
		"connect-flash": "^0.1.1",
		"cookie-parser": "^1.4.6",
		"correios-brasil": "^3.0.3",
		"cors": "^2.8.5",
		"csurf": "^1.11.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"express-recaptcha": "^5.1.0",
		"express-session": "^1.17.3",
		"express-validator": "^6.14.2",
		"fs-extra": "^11.1.0",
		"handlebars": "^4.7.7",
		"helmet": "^6.0.1",
		"https-localhost": "^4.7.1",
		"jsonwebtoken": "^9.0.0",
		"mustache": "^4.2.0",
		"mustache-express": "^1.3.2",
		"node-cache": "^5.1.2",
		"node-fb-login": "^1.0.0",
		"node-fetch": "^3.3.0",
		"node-google-login": "^1.0.1",
		"nodemailer": "^6.8.0",
		"pagination": "^0.4.6",
		"pg": "^8.8.0",
		"pg-hstore": "^2.3.4",
		"query-string": "^8.1.0",
		"rand-token": "^1.0.1",
		"slugify": "^1.6.5",
		"stripe": "^11.5.0",
		"uuid": "^9.0.0",
		"validator": "^13.7.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.59",
		"@swc/core": "^1.3.24",
		"@types/bcrypt": "^5.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/compression": "^1.7.2",
		"@types/connect-flash": "^0.0.37",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/csurf": "^1.11.2",
		"@types/express": "^4.17.15",
		"@types/express-session": "^1.17.5",
		"@types/fs-extra": "^9.0.13",
		"@types/jsonwebtoken": "^9.0.0",
		"@types/mime": "^3.0.1",
